  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'rust-rtow'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=rust-rtow"],
        "filter": {
          "name": "rust-rtow",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rust-rtow'",
      "cargo": {
        "args": ["build", "--bin=rust-rtow", "--package=rust-rtow"],
        "filter": {
          "name": "rust-rtow",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rust-rtow'",
      "cargo": {
        "args": ["test", "--no-run", "--bin=rust-rtow", "--package=rust-rtow"],
        "filter": {
          "name": "rust-rtow",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
